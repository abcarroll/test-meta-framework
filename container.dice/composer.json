{
  "name":         "abc/container.dice",
  "type":         "library",
  "license":      "MIT",
  "description":  "The interop adapter for snapping the hassankhan/config implementation into the abc/config interface",
  "keywords": [
    "interface",
    "interop",
    "contract",
    "framework",
    "scaffolding",
    "kernel",
    "bootstrap"
  ],
  "suggests": {
    "abc/container.dice.*": "Concrete adapter implementations for the abc/container.dice interop interface"
  },
  "authors": [
    {
      "name":     "A.B. Carroll",
      "email":    "ben@hl9.net",
      "homepage": "http://abphp.hl9.net"
    }
  ],
  "require": {
    "abc/container.dice": "*",
    "psr/container": "^1.0",
    "level-2/dice": "^2.0",
    "php": ">=5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "abc\\Container.dice\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "abc\\Container.dice\\Tests\\": "tests/"
    }
  }
}