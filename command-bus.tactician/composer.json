{
  "name":         "abc/command-bus.tactician",
  "type":         "library",
  "license":      "MIT",
  "description":  "The interop adapter for snapping league/tacticin implementation into the abc/command-bus interface.",
  "keywords": [
    "interface",
    "interop",
    "contract",
    "framework",
    "scaffolding",
    "kernel",
    "bootstrap"
  ],
  "suggests": {
    "abc/queue": "Queue interface that can snap into abc/command-bus"
  },
  "authors": [
    {
      "name":     "A.B. Carroll",
      "email":    "ben@hl9.net",
      "homepage": "http://abphp.hl9.net"
    }
  ],
  "require": {
    "abc/command-bus": "dev-master",
    "league/tactician": "^1.0",
    "league/tactician-bernard": "^0.6.0",
    "league/tactician-container": "^2.0",
    "league/tactician-doctrine": "^1.1",
    "league/tactician-bundle": "^0.4.1",
    "league/tactician-logger": "^0.10.0",
    "php": ">=5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.3",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "psr-4": {
      "abc\\CommandBus\\Adapter\\Tactician\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "abc\\CommandBus\\Tests\\": "tests/"
    }
  }
}